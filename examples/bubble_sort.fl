array = [5, 3, 7, 10]
array_len = 4
.. Bubble sort
i = 0
w i ls array_len
{
    j = 0
    w j ls array_len
    {
        if array[i] ls array[j]
        {
            temp = array[i]
            array[i] = array[j]
            array[j] = temp
        }
        j += 1
    }
    i += 1 
}

i = 0
w i ls array_len
{
    p array[i]
    i += 1
}